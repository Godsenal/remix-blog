{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate && remix build",
    "dev": "node -r dotenv/config node_modules/.bin/remix dev",
    "lint": "eslint app/**/*.{js,jsx,ts,tsx} && prettier --write app/**/*.{js,jsx,ts,tsx}",
    "gen:theme-typings": "chakra-cli tokens app/theme.ts",
    "postinstall": "remix setup node && npm run gen:theme-typings",
    "start": "remix-serve build",
    "deploy": "fly deploy --remote-only",
    "prisma:generate": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate",
    "prisma:push": "node scripts/prisma_origin db push"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@prisma/client": "^3.8.1",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@tiptap/extension-code-block-lowlight": "^2.0.0-beta.68",
    "@tiptap/extension-image": "^2.0.0-beta.25",
    "@tiptap/extension-placeholder": "^2.0.0-beta.47",
    "@tiptap/react": "^2.0.0-beta.107",
    "@tiptap/starter-kit": "^2.0.0-beta.180",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "dotenv": "^14.2.0",
    "framer-motion": "^5.6.0",
    "lowlight": "^2.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "remix": "^1.1.1",
    "tiny-invariant": "^1.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.7.0",
    "@remix-run/dev": "^1.1.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prisma": "^3.8.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
